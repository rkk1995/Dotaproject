<html lang="en" xmlns="http://www.w3.org/1999/html">
<head>
    <link rel="shortcut icon" type="image/x-icon" href="/static/images/favicon.ico.png">
    <script src="/static/js/jquery.js"></script>
    <script src="/static/js/custom.js"></script>
    <meta charset="UTF-8">
    <title>Dota2ProTracker</title>
    <!--- <link rel=stylesheet type=text/css href="/static/light.css"> -->
    <link rel=stylesheet type=text/css href="/static/dark.css">
</head>

<body>

<div id="container">

    <div id="header">
        <a href="#" id="toggle"></a>
        <a href="{% url 'protracker:index' %}"  >
            <img src="/static/images/logo_new.png"/>
        </a>
    </div>

    <div id="left" class="column">
        <div id="left_head">
            Live Games With Pros
        </div>
        {% for match in livematches %}

            <div class="live_match">

                <div class="live_match_info">
                    <div>{{match.average_mmr}} <span>avg MMR</span></div>
                    
                    {% if match.game_time <= 10 %}
                        drafting
                    {% else %}
                        {{match.game_time}} 
                    {% endif %}
                </div>

                <div class="live_match_players">
                    <div class="live_match_player">
                        <div class="live_match_player_portrait radiant">
                            
                                <a href="/hero/Nature's Prophet" title="Nature's Prophet">
                                    <img src="http://cdn.dota2.com/apps/dota2/images/heroes/furion_vert.jpg">
                                </a>
                            
                        </div>
                        <div class="live_match_player_info radiant">
                            <div class="live_match_player_name">
                                <a href="/player/monkeys-forever" title="monkeys-forever">monkeys-forever</a>
                                <span>
                                    
                                        Nature's Prophet
                                    
                                </span>
                            </div>
                            <div class="live_match_player_stats">4/6/11</div>
                            <div class="live_match_player_level">Level 19</div>
                            <div class="live_match_player_side_icon"><img src="/static/images/radiant.png"></div>
                        </div>
                    </div>
                
                    <div class="live_match_player">
                        <div class="live_match_player_portrait dire">
        
                                <a href="/hero/Phoenix" title="Phoenix">
                                    <img src="http://cdn.dota2.com/apps/dota2/images/heroes/phoenix_vert.jpg">
                                </a>
                            
                        </div>
                        <div class="live_match_player_info dire">
                            <div class="live_match_player_name">
                                <a href="/player/ixmike88" title="ixmike88">ixmike88</a>
                            <span>
                                    
                                        Phoenix
                                    
                                </span>
                            </div>
                            <div class="live_match_player_stats">2/9/17</div>
                            <div class="live_match_player_level">Level 15</div>
                            <div class="live_match_player_side_icon"><img src="/static/images/dire.png"></div>
                        </div>
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>

</div>



</body>
